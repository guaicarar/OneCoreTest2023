@model List <OneCoreTest2023.Models.ViewModels.ListClienteViewModel>
@{
    ViewBag.Title = "DATOS CLIENTES";
}

<h2>View Datos Usuarios</h2>
<div claas="row" style="text-align:right;padding:5px;">
    <a href="~/Cliente/Nuevo" class="btn btn-primary"> Nuevo</a>
</div>
<div class=" row">
    <div class="col-md-12">
        <table class="table">
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>RFC</th>
                <th>Dirección</th>
                <th>CP</th>
                <th>Correo</th>
                <th></th>
            </tr>
            @foreach (var oElemento in Model)
            {
                <tr>
                    <td>@oElemento.ID</td>
                    <td>@oElemento.NOMBRE</td>
                    <td>@oElemento.RFC</td>
                    <td>@oElemento.DIRECCION</td>
                    <td>@oElemento.CP</td>
                    <td>@oElemento.CORREO</td>
                    <td>
                        <a class=" btn btn-default" href=" ~/Cliente/Editar/@oElemento.ID"> Editar</a>
                        <a class=" btn btn-danger" href=" ~/Cliente/Eliminar/@oElemento.ID"> Eliminar</a>
                    </td>

                </tr>
            }
        </table>

    </div>
</div>

